<?xml version="1.0" encoding="utf-8"?>
<layer-list xmlns:android="http://schemas.android.com/apk/res/android">

    <item android:id="@+id/ISeekBarBackground">

        <shape>
            <corners android:radius="@dimen/dimen_corner_4" />
            <size android:height="@dimen/dimen_2" />
            <solid android:color="@color/color_gray_player" />
        </shape>

    </item>

    <item android:id="@+id/ISeekBarProgress">

        <clip>

            <shape android:shape="rectangle">
                <corners android:radius="@dimen/dimen_corner_4" />
                <size android:height="@dimen/dimen_2" />
                <solid android:color="@color/color_black_player" />
            </shape>

        </clip>

    </item>

</layer-list>
